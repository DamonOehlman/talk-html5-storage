{
    "title": "The Art of HTML5 Storage",
    "template": "github://DamonOehlman/packing-instructions/portable",
    "sources": [
        "files/intro.md",
        "files/webstorage.md",
        "files/websql.md",
        "files/indexeddb.md",
        "files/wrappers.md",
        "files/art.md",
        "files/outro.md"
    ],
    
    "includes": {
        "head": [
            "css/pres.style.css"
        ],
        
        "body": [
            "js/pres.js"
        ]
    },
    
    "theme": "dark",
    
    "aliases": {
        "deck": "github://imakewebthings/deck.js"
    },
    
    "plugins": {
        "code": {
            "style": "zenburn"
        }
    },
    
    "resources": [
        "css/swiss.css <= deck!themes/style/swiss.css",
        "css/deck.hash.css <= deck!extensions/hash/deck.hash.css",
        "css/pres.style.css <= resources/css/pres.style.css",
        "js/deck.hash.js <= deck!extensions/hash/deck.hash.js",
        "js/pres.js <= resources/js/pres.js",
        "js/jquery.min.js <= http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"
    ]
}